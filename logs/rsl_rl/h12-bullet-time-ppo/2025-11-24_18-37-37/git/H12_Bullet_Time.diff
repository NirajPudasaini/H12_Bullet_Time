+def projectile_proximity_penalty(
+    env: ManagerBasedRLEnv,
+    asset_cfg: SceneEntityCfg,
+    projectile_name: str = "Projectile",
+    max_distance: float = 2.0,
+    penalty_scale: float = -1.0,
+) -> torch.Tensor:
+    """Continuous penalty for projectile proximity (discourage close approaches).
+    
+    Returns penalty proportional to how close projectile is.
+    Useful in Phase 2a to encourage proactive dodging without hard termination.
+    
+    Args:
+        env: RL environment
+        asset_cfg: Robot entity config
+        projectile_name: Name of projectile entity
+        max_distance: Distance at which penalty reaches maximum (meters)
+        penalty_scale: Penalty coefficient (negative, e.g., -1.0)
+    
+    Returns:
+        Tensor of shape (num_envs,) with continuous penalty based on distance
+    """
+    # Get robot
+    robot: Articulation = env.scene[asset_cfg.name]
+    robot_pos = robot.data.root_pos_w[:, :3]  # shape: (num_envs, 3)
+    
+    # Get projectile
+    try:
+        projectile = env.scene[projectile_name]
+        proj_pos = projectile.data.root_pos_w  # shape: (num_envs, 3)
+    except (KeyError, AttributeError):
+        # Projectile not found, no penalty
+        return torch.zeros(env.num_envs, dtype=torch.float32, device=env.device)
+    
+    # Compute distance from projectile to robot base
+    distance = torch.norm(proj_pos - robot_pos, dim=-1)  # shape: (num_envs,)
+    
+    # Linear penalty: closer = more penalty
+    # Ramps from 0 at max_distance to full penalty_scale at distance=0
+    # Beyond max_distance, penalty is 0
+    penalty = torch.clamp(
+        float(penalty_scale) * (1.0 - distance / float(max_distance)),
+        min=float(penalty_scale),  # Clamp to max penalty
+        max=0.0  # Never give positive reward
+    )
+    
+    return penalty
+
+
+
+def projectile_distance(
+    env: ManagerBasedRLEnv,
+) -> torch.Tensor:
+    """Distance-based reward for keeping projectile far away.
+    
+    Reward structure:
+    - Distance > 3m: Linear reward for excess distance (1 point per 0.1m beyond 3m)
+    - Distance 2-3m: No reward/penalty (neutral zone)
+    - Distance < 2m: Hard penalty (-10.0)
+    
+    This encourages the robot to:
+    1. Maintain minimum safe distance of 2m
+    2. Get extra reward for staying >3m away
+    3. Get heavily penalized for letting projectile get too close
+    """
+    try:
+        projectile = env.scene["Projectile"]
+        proj_pos = projectile.data.root_pos_w  # shape: (num_envs, 3)
+    except (KeyError, AttributeError):
+        # Projectile not spawned yet, no reward
+        return torch.zeros(env.num_envs, dtype=torch.float32, device=env.device)
+    
+    # Get robot base position
+    robot = env.scene["robot"]
+    robot_pos = robot.data.root_pos_w  # shape: (num_envs, 3)
+    
+    # Compute distance
+    distance = torch.norm(proj_pos - robot_pos, dim=-1)  # shape: (num_envs,)
+    
+    # Initialize reward tensor
+    reward = torch.zeros(env.num_envs, dtype=torch.float32, device=env.device)
+    
+    # Hard penalty for distance < 2m (too close!)
+    too_close = distance < 2.0
+    reward[too_close] = -10.0
+    
+    # Neutral zone for 2m <= distance <= 3m (safe, but no bonus)
+    # (no change needed, already 0)
+    
+    # Linear reward for distance > 3m (extra distance = bonus)
+    # Reward = (distance - 3.0) for each meter beyond 3m
+    far = distance >= 3.0
+    reward[far] = (distance[far] - 3.0)  # Linear excess distance reward
+    
+    return reward
+
